    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//FPSController_step();
if (rdy)
    {
    //Check audio sync
    if (rdy and play and songloaded and (((real(saud_position(audio))/1000*showfps - frame) &gt; 3 or (real(saud_position(audio))/1000*showfps - frame) &lt; -3))) 
        frame = floor(real(saud_position(audio))/1000*showfps);
    
    //Clamp room size
    if (rheight &lt; 100)
        rheight = 100;
    if (rwidth &lt; 200)
        rwidth = 200;
    if (rlength &lt; 200)
        rlength = 200;
    
    //Increase/decrease frame number
    if (keyboard_check(vk_right))
        {
        if (frame &lt; ds_list_size(ild_list)-6) frame++; //read_frame();
        }
    if (keyboard_check(vk_left))
        {
        if (frame) frame--; //read_frame();
        }
    
    //Toggle 3D/2D
    if (keyboard_check_pressed(vk_tab))
        {
        if (ds_list_find_value(scan_list,0))
            {
            if (!room) room_goto_next(); 
            else room_goto_previous();
            }
        else show_message("Please load at least a main ILDA file before entering 3D mode");
        }
        
    //IPD
    if (keyboard_check_direct(ord("L"))) {
        GMO_setIPD(GMO_getIPD()+0.01);
    }
    if (keyboard_check_direct(ord("K"))) {
        GMO_setIPD(GMO_getIPD()-0.01);
    }
        
    //Play/pause
    if (keyboard_check_pressed(vk_space))
        {
        if (saud_is_playing(audio))
            {
            saud_stop(audio);
            }
        else
            {
            saud_seek(floor(frame*1000/showfps),audio);
            }
        play = !play;
        if (play)
            alarm[3]=1;
        }
    
    //Stop and reset
    if (keyboard_check_pressed(vk_backspace))
        {
        frame = 0;
        saud_stop(audio);
        play = 0;
        }
            
    //Jump to frame
    if (keyboard_check_pressed(ord('N')))
        {
        seekto = get_integer("Enter frame number to skip to, 0 to "+string(ds_list_size(ild_list)-6),0);
        if ((seekto &gt;= 0) and (seekto &lt;= (ds_list_size(ild_list)-6)))
            {
            saud_stop(audio);
            play = 0;
            frame = seekto;
            }
        if (room)
            window_set_fullscreen(1);
        }
    
    //screenshot
    if (keyboard_check_pressed(ord('M')))
        {
        ssfilename = get_save_filename_ext("*","*",working_directory,"Save screenshot");
        screen_save(ssfilename);
        if (room)
            window_set_fullscreen(1);
        }
        
    //Toggle fog
     if (keyboard_check_pressed(ord('B')) and room and rdy)
        {
        fog = !fog;
        }
    
    //Check fps
    if (room)
        {
        
        if (rdy and play and fps &gt; 73)
            {
            fpswarning = 1;
            alarm[0] = 500;
            }
        }
        
    }
    
    
    

</string>
          </argument>
        </arguments>
      </action>
